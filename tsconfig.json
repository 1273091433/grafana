{
    "compilerOptions": {
        "declaration": true,
        "outDir": "public_gen",
        "noImplicitAny": false,
        "target": "es5",
        "lib": ["es6", "dom"],
        "rootDir": "public/",
        "module": "es6",
        "jsx": "react",
        "inlineSourceMap": false,
        "sourceMap": true,
        "noEmitOnError": false,
        "emitDecoratorMetadata": false,
        "experimentalDecorators": false,
        "noImplicitReturns": true,
        "noImplicitThis": false,
        "noImplicitUseStrict":false,
        "moduleResolution": "node",
        "baseUrl": "public",
        "paths": {
          "app": ["app"]
        }
    },
    "include": [
        "public/app/**/*.ts",
        "public/app/**/*.tsx",
        "public/test/**/*.ts"
    ],
    "exclude": [
    ]
}
